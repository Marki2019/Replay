#. Translators: This is the title of the general shortcuts group
#: data/gtk/help-overlay.ui:32
msgid "General"
msgstr "General"

#. Translators: This is the title for the shortcut that actually shows the shortcuts window
#: data/gtk/help-overlay.ui:37
msgid "Show shortcuts"
msgstr "Mostrar atajos"

#. Translators: This is the title for the shortcut that quits the app
#: data/gtk/help-overlay.ui:45
msgid "Quit"
msgstr "Salir"

#. Translators: This is the title for the shortcut that closes main window without quit the app
#: data/gtk/help-overlay.ui:53
msgid "Close window"
msgstr "Cerrar ventana"

#. Translators: This is the title for the shortcut that opens the search bar
#. Translators: This is the text for the tooltip on the search button
#: data/gtk/help-overlay.ui:61 data/gtk/main-header-bar.ui:78
msgid "Search"
msgstr "Buscar"

#. Translators: This is the title for the shortcut that opens the primary menu
#: data/gtk/help-overlay.ui:69
msgid "Open/close primary menu"
msgstr "Abrir/cerrar menú primario"

#. Translators: This is the app name
#: data/gtk/about-dialog.ui:27 data/gtk/main-header-bar.ui:42
#: data/gtk/main-window.ui:26 data/com.github.nahuelwexd.Replay.desktop.in.in:5
#: data/com.github.nahuelwexd.Replay.metainfo.xml.in.in:5
msgid "Replay"
msgstr "Replay"

#. Translators: This is the comment for the About dialog
#. Translators: This is the first paragraph of the description of the app. This will be shown on GNOME Software or the Flathub website
#: data/gtk/about-dialog.ui:31
#: data/com.github.nahuelwexd.Replay.metainfo.xml.in.in:10
msgid "An open source YouTube client for GNOME."
msgstr "Un cliente de YouTube de código abierto para GNOME"

#. Translators: This is the label used to link to the project repo
#: data/gtk/about-dialog.ui:33
msgid "Project repository"
msgstr "Repositorio del proyecto"

#. Translators: This is the text for the tooltip on the primary menu button
#: data/gtk/main-header-bar.ui:94
msgid "Menu"
msgstr "Menú"

#. Translators: This is the label used for the preferences item in the primary menu
#: data/gtk/main-header-bar.ui:110 data/gtk/preferences-window.ui:25
msgid "Preferences"
msgstr "Preferencias"

#. Translators: This is the label used for the keyboard shortcuts item in the primary menu
#: data/gtk/main-header-bar.ui:116
msgid "Keyboard Shortcuts"
msgstr "Atajos de teclado"

#. Translators: This is the label used for the about item in the primary menu
#: data/gtk/main-header-bar.ui:122
msgid "About Replay"
msgstr "Acerca de Replay"

#. Translators: This is the title for the trending section of the app
#: data/gtk/main-window.ui:68
msgid "Trending"
msgstr "Tendencias"

#. Translators: This is the title for the subscriptions section of the app
#: data/gtk/main-window.ui:82
msgid "Subscriptions"
msgstr "Suscripciones"

#. Translators: This is the title for the library section of the app
#: data/gtk/main-window.ui:96
msgid "Library"
msgstr "Biblioteca"

#. Translators: This is the title for the appearance section on the preferences window
#: data/gtk/preferences-window.ui:32
msgid "Appearance"
msgstr "Apariencia"

#. Translators: This is the title for the dark theme toggle in the preferences window
#: data/gtk/preferences-window.ui:45
msgid "Use dark theme"
msgstr "Usar tema oscuro"

#. Translators: This is the subtitle for the dark theme toggle in the preferences window
#: data/gtk/preferences-window.ui:47
msgid "Activates dark theme on the app."
msgstr "Activa el tema oscuro en la aplicación"

#. Translators: This is the generic app name
#: data/com.github.nahuelwexd.Replay.desktop.in.in:7
msgid "YouTube client"
msgstr "Cliente de YouTube"

#. Translators: This is the comment of the app
#: data/com.github.nahuelwexd.Replay.desktop.in.in:9
msgid "View YouTube videos"
msgstr "Ver videos de YouTube"

#. Translators: This is the app icon, this MUST NOT be translated
#: data/com.github.nahuelwexd.Replay.desktop.in.in:11
msgid "@APPLICATION_ID@"
msgstr "@APPLICATION_ID@"

#. Translators: These are the keywords used for the search trackers. The list MUST be delimited with semicolons, and MUST finish with a semicolon
#: data/com.github.nahuelwexd.Replay.desktop.in.in:17
msgid "YouTube;Videos;"
msgstr "YouTube;Videos;"

#. Translators: This is the summary of the app that will be shown on GNOME Software or the Flathub website
#: data/com.github.nahuelwexd.Replay.metainfo.xml.in.in:7
msgid "An open source YouTube client for GNOME"
msgstr "Un cliente de YouTube de código abierto para GNOME"

#. Translators: This is the name of the developer, it should not be translated
#: data/com.github.nahuelwexd.Replay.metainfo.xml.in.in:16
msgid "Nahuel Gomez Castro"
msgstr "Nahuel Gomez Castro"

#. Translators: This is the caption of the first "screenshot", which is a mockup at the moment
#: data/com.github.nahuelwexd.Replay.metainfo.xml.in.in:21
msgid "Mockup of the app"
msgstr "Boceto de la aplicación"
